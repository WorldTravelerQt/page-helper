<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- 绑定一个对应的Dao/Mapper接口 -->
<mapper namespace="com.scxh.pagehelper.mapper.BookMapper">
    <!--
        select查询语句
        id:必须与接口方法对应！！
        resultType：结果集封装类型
     -->
    <select id="findBookList" resultType="com.scxh.pagehelper.entity.Book">
        select * from books
    </select>
</mapper>
